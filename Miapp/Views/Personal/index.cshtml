
@{
    ViewData["Title"] = "Sobre mí";
}
<link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
<link rel="stylesheet" href="~/css/site.css" />
<div class="p-3 mb-2 bg-info text-white">
<div class="text-center">
<div class="alert alert-secondary" role="alert">
  <h1>Datos personales</h1>
</div>
</div>


<div class="card text-white bg-dark mb-3" style="width: 20rem;">
  <img src="img/foto.jpeg" class="card-img-top" alt="Mi imagen">
  <div class="card-body">
    <div class="text-left">
    <p class="card-text">
@model Persona

@Html.LabelFor(m=>m.nombre, "NOMBRE: ") @Model.nombre <br>
@Html.LabelFor(m=>m.apellido, "APELLIDO: ") @Model.apellido <br>
@Html.LabelFor(m=>m.edad, "EDAD: ") @Model.edad <br>
@Html.LabelFor(m=>m.correo, "CORREO ELECTRÓNICO: ") @Model.correo <br><br>
@Html.LabelFor(m=>m.telefono, "TELÉFONO: ") @Model.telefono <br>
@Html.LabelFor(m=>m.direccion, "DIRECCIÓN: ") @Model.direccion <br>
@Html.LabelFor(m=>m.estado, "ESTADO CIVIL: ") @Model.estado <br>
</p>
  </div>
  </div>
</div>
<div class="card text-white bg-dark mb-3" style="width: 20rem;">
  <img src="img/Adony.PNG" class="card-img-top" alt="Mi imagen">
  <div class="card-body">
    <div class="text-left">
    <p class="card-text">

  @Html.LabelFor(m=>m.nombre, "NOMBRE: Edwin Adony ")  <br>
  @Html.LabelFor(m=>m.apellido, "APELLIDO: Ulloa Diaz") <br>
  @Html.LabelFor(m=>m.edad, "EDAD: 25") <br>
  @Html.LabelFor(m=>m.correo, "CORREO ELECTRÓNICO: adonyulloa@gmail.com ")  <br><br>
  @Html.LabelFor(m=>m.telefono, "TELÉFONO: 7721-9272") <br>
  @Html.LabelFor(m=>m.direccion, "DIRECCIÓN: San Miguel, San Miguel") <br>

  
</div>

